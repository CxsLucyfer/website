    each val in reviews

      .py-12.px-4(class='sm:px-6 md:flex md:flex-col md:py-16 md:pl-0 md:pr-10 md:border-r md:border-indigo-900 lg:pr-16')
        div(class='md:flex-shrink-0')
          img.h-12(src= val.photo alt='Tuple')
        blockquote.mt-6(class='md:flex-grow md:flex md:flex-col')
          .relative.text-lg.font-medium.text-white(class='md:flex-grow')
            svg.absolute.top-0.left-0.transform.-translate-x-3.-translate-y-2.h-8.w-8.text-indigo-600(fill='currentColor' viewbox='0 0 32 32' aria-hidden='true')
